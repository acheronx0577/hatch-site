{
  "name": "@hatch/api",
  "private": true,
  "scripts": {
    "dev": "nest start --watch --entryFile apps/api/src/main",
    "build": "pnpm run prisma:generate && pnpm exec nest build",
    "build:dist": "pnpm run prisma:generate && pnpm exec nest build",
    "prisma:generate": "pnpm --filter @hatch/db run generate && node ./scripts/sync-prisma-client.js",
    "start": "node dist/apps/api/src/main.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "sendgrid:smoke": "ts-node ./scripts/sendgrid-smoke.ts",
    "ai-memory:backfill": "ts-node ./scripts/backfill-ai-memory.ts",
    "ai-embeddings:reembed": "ts-node ./scripts/reembed-embeddings.ts",
    "ingest:forms": "ts-node --transpile-only -r tsconfig-paths/register src/scripts/ingest-forms.ts",
    "public-records:sync": "ts-node --transpile-only -r tsconfig-paths/register src/scripts/public-records-sync.ts",
    "seed:demo": "ts-node scripts/seed-demo-data.ts",
    "agent-performance:recompute": "ts-node -r tsconfig-paths/register scripts/recompute-agent-performance.ts"
  },
  "dependencies": {
